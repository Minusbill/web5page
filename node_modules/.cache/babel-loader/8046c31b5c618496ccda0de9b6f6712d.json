ectedCategory.value) {\n    return filterProjectsByCategory();\n  } else if (searchProject.value) {\n    return filterProjectsBySearch();\n  }\n  return projects.value;\n});\n\n// Methods\nfunction filterProjectsByCategory() {\n  return projects.value.filter((item) => {\n    let category = item.category.charAt(0).toUpperCase() + item.category.slice(1);\n    return category.includes(selectedCategory.value);\n  });\n}\n\nfunction filterProjectsBySearch() {\n  let project = new RegExp(searchProject.value, 'i');\n  return projects.value.filter((el) => el.title.match(project));\n}\n\nonMounted(() => {\n  feather.replace();\n});\n</script>\n\n\n<template>\n\t<!-- Projects grid -->\n\t<section class=\"pt-10 sm:pt-14\">\n\t\t<!-- Projects grid title -->\n\t\t<div class=\"text-center\">\n\t\t\t<p\n\t\t\t\tclass=\"font-general-semibold text-2xl sm:text-5xl font-semibold mb-2 text-ternary-dark dark:text-ternary-light\"\n\t\t\t>\n\t\t\t\t{{ projectsHeading }}\n\t\t\t</p>\n\t\t</div>\n\n<!--\t\t&lt;!&ndash; Filter and search projects &ndash;&gt;-->\n<!--\t\t<div class=\"mt-10 sm:mt-10\">-->\n<!--\t\t\t<h3-->\n<!--\t\t\t\tclass=\"font-general-regular-->\n<!--\t\t\t\t\ttext-center text-secondary-dark-->\n<!--\t\t\t\t\tdark:text-ternary-light-->\n<!--\t\t\t\t\ttext-md-->\n<!--\t\t\t\t\tsm:text-xl-->\n<!--\t\t\t\t\tfont-normal-->\n<!--\t\t\t\t\tmb-4-->\n<!--\t\t\t\t\"-->\n<!--\t\t\t>-->\n<!--\t\t\t</h3>-->\n<!--\t\t\t<div-->\n<!--\t\t\t\tclass=\"-->\n<!--\t\t\t\t\tflex-->\n<!--\t\t\t\t\tjustify-between-->\n<!--\t\t\t\t\tborder-b border-primary-light-->\n<!--\t\t\t\t\tdark:border-secondary-dark-->\n<!--\t\t\t\t\tpb-3-->\n<!--\t\t\t\t\tgap-2-->\n<!--\t\t\t\t\"-->\n<!--\t\t\t>-->\n<!--&lt;!&ndash;\t\t\t\t<div class=\"flex justify-between gap-2\">&ndash;&gt;-->\n<!--&lt;!&ndash;\t\t\t\t\t<span&ndash;&gt;-->\n<!--&lt;!&ndash;\t\t\t\t\t\tclass=\"&ndash;&gt;-->\n<!--&lt;!&ndash;\t\t\t\t\t\t\thidden&ndash;&gt;-->\n<!--&lt;!&ndash;\t\t\t\t\t\t\tsm:block&ndash;&gt;-->\n<!--&lt;!&ndash;\t\t\t\